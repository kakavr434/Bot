import discord
import random
# ayricaliklar (intents) deikeni botun ayr覺cal覺klar覺n覺 depolayacak
intents = discord.Intents.default()
# Mesajlar覺 okuma ayr覺cal覺覺n覺 etkinletirelim
intents.message_content = True
# client (istemci) deikeniyle bir bot olutural覺m ve ayr覺cal覺klar覺 ona aktaral覺m
client = discord.Client(intents=intents)

@client.event
async def on_ready():
    print(f'{client.user} olarak giri yapt覺k.')

@client.event
async def on_message(message):
    if message.author == client.user:
        return
    if message.content.startswith('merhaba'):
        await message.channel.send("Selam!")
    elif message.content.startswith('bye'):
        await message.channel.send("\U0001f642")
    elif message.content.startswith('ifre'):
        passwords = ("")
        elements = "1234567890qwertyu覺opasdfghjkl羹izxcvbnm繹癟+-/*!&$#?=@<>"
        for i in range(8):
            passwords += random.choice(elements)
        await message.channel.send(passwords)
    elif message.content.startswith("naber"):
        await message.channel.send("襤yiyim senden naber")
    else:
        await message.channel.send("Umm what the Sigma")
        await message.channel.send("")
    

client.run("Your Token(Discord)")
